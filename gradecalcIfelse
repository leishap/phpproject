    <body>
       <?php
       $grade=85;
       if($grade>=90){
       echo"You got an A!";
       }
       else if($grade>=80){
           echo"You got a B!";
           }
           else if($grade>=70){
               echo"You got a C!";
           }
           else if ($grade>=60){
               echo"You got a D, better try harder!";
           }
           else
           echo"Hey big guy, You failed! Try again!"
           ?>
    </body>
